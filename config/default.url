      <servlet_url servlet="UserController" method="userLogin">      /userLogin.svc      </servlet_url>
      <servlet_url servlet="UserController" method="userLoginView">  /userLoginView.svc  </servlet_url>
   </servlet_url_list>
   
   <servlet_url_list url-prefix="" package="community.common.controller">
      <servlet_url servlet="MainController" method="mainView">  /main.svc      </servlet_url>
      <servlet_url servlet="MainController" method="topView">        /top.svc            </servlet_url>
      <servlet_url servlet="MainController" method="mainFrameView">       /mainFrame.svc           </servlet_url>
   </servlet_url_list>

