<?xml version="1.0" encoding="UTF-8"?>
<jpdk_servlet_url>

   <servlet_url_list url-prefix="/admin" package="community.admin.controller">
      <servlet_url servlet="AdminController" method="adminMenuView"> /menu.svc </servlet_url>
   </servlet_url_list>
   
   <servlet_url_list url-prefix="/board" package="community.board.controller">
      <servlet_url servlet="BoardController" method="communityView"> /community.svc </servlet_url>
      <servlet_url servlet="BoardController" method="boardWriteView">/write.svc </servlet_url>
      <servlet_url servlet="BoardController" method="saveContent">/saveContent.svc </servlet_url>
      <servlet_url servlet="BoardController" method="contentview">/contentview.svc </servlet_url>
      <servlet_url servlet="BoardController" method="loadSmartEditorSkin">/loadEditor.svc</servlet_url>
      <servlet_url servlet="BoardController" method="loadInputArea">/load_inputarea.svc</servlet_url>
      <servlet_url servlet="BoardController" method="loadInputArea8">/load_inputarea_ie8.svc</servlet_url>
      <servlet_url servlet="BoardController" method="photoUpload">/photo_upload.svc</servlet_url>
